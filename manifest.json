{
    "name": "Chromi3wm",
    "version": "0.5",
    "description": "Connects to i3.",
    "icons": {
        "16": "logo/16.png",
        "32": "logo/32.png",
        "48": "logo/48.png",
        "128":"logo/128.png" },
    "browser_action": {
        "default_icon": {
            "19": "logo/19.png",
            "38": "logo/38.png" },
        "default_title": "Connect to i3" },
    "background": {
        "persistent": false,
        "scripts": ["background.js"] },
    "permissions": [ "nativeMessaging" ],
    "minimum_chrome_version": "39",
    "manifest_version": 2
}
